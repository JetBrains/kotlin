    val list = list(b, a, c, d, e)
    b = list[0]
    c = list[1]
    d = list[2]
    e = list[3]

    return b + c + d + e
}

private fun list(b: Int, a: Int, c: Int, d: Int, e: Int): List<Int> {
    var b1 = b
    var c1 = c
    var d1 = d
    var e1 = e
    b1 += a
    c1 -= a
    d1 += c1
    e1 -= d1
    println(b1)
    println(c1)
