// SIBLING:
fun foo(a: Int): Int {
    var b: Int = 1
    var c: Int = 1
    var d: Int = 1

    val triple = triple(b, a, c, d)
    b = triple.first
    c = triple.second
    d = triple.third

    return b + c + d
}

private fun triple(
    b: Int,
    a: Int,
    c: Int,
    d: Int
): Triple<Int, Int, Int> {
    var b1 = b
    var c1 = c
    var d1 = d
    b1 += a
    c1 -= a
    d1 += c1
    println(b1)
    println(c1)
    return Triple(b1, c1, d1)
}