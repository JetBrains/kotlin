// INTENTION_TEXT_2: "Replace with 'asSequence().mapIndexed{}.forEach{}'"
fun foo(list: List<String>) {
    list
            .asSequence()
            .mapIndexed { index, s -> s.length * index }
            .forEach { println(it) }
}