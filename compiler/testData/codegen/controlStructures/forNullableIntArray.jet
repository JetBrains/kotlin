fun box() : String {
    val b : Array<Int?> = arrayOfNulls<Int> (5)
    var i = 0
    var sum = 0
    while(i < 5) {
       b[i] = i++
    }
    sum = 0
    for (el in b) {
        sum = sum + (el ?: 0)
    }
    System.out?.println(sum)
    if(sum != 10) return "b failed"

    val d : Array<Int?>? = null
    for (el in d) {
        sum = sum + (el ?: 7)
    }

    return "OK"
}