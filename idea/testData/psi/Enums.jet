enum class Color(val rgb : Int) {
    RED : Color(0xFF000)
    GREEN : Color(0x00FF00)
    BLUE : Color(0x0000FF)
}

enum class List<out T>(val size : Int) {
    Nil : List<Nothing>(0)

    Cons<T>(value : T, tail : List<T>) : List<T>(/*tail.size + 1*/a)

    val isEmpty : Bool
      get() = size //> 0
}