// INTENTION_TEXT_2: "Replace with 'asSequence().filterNot{}.mapIndexed{}.firstOrNull{}'"
fun foo(list: List<String>): Int? {
    return list
        .filterNot { it.isBlank() }
        .mapIndexed { index, s -> s.length * index }
        .firstOrNull { it > 0 }
}