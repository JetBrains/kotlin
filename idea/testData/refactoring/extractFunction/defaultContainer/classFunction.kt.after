// PARAM_TYPES: kotlin.Int
// PARAM_TYPES: kotlin.Int
// PARAM_DESCRIPTOR: value-parameter val a: kotlin.Int defined in A.B.foo
// PARAM_DESCRIPTOR: value-parameter val b: kotlin.Int defined in A.B.foo
class A {
    class B {
        fun foo(a: Int, b: Int): Int {
            return i(a, b)
        }

        private fun i(a: Int, b: Int) = a + b - 1
    }
}