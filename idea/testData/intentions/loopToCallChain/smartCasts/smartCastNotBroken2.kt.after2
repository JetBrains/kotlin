// INTENTION_TEXT_2: "Replace with 'asSequence().map{}.map{}.firstOrNull{}'"
fun foo(list: List<String>, o: Any): Int? {
    if (o is CharSequence) {
        <caret>return list
            .asSequence()
            .map { it.length + (o as String).capitalize().hashCode() }
            .map { it * o.length }
            .firstOrNull { it > 1000 }
    }
    return 0
}