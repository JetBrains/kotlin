// INTENTION_TEXT: "Replace with '...filter{}.map{}.firstOrNull()'"
// INTENTION_TEXT_2: "Replace with 'asSequence()...filter{}.map{}.firstOrNull()'"
fun foo(list: List<String>, o: Any): Int? {
    if (o is Int) {
        return list
                .map { it.length + o }
                .filter { it > 0 }
                .map { it * o.hashCode() }
                .firstOrNull()
    }
    return 0
}