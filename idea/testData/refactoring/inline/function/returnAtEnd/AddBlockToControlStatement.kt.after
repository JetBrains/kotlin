fun main(args: Array<String>) {
    if (args.size > 0) {
        println(1)
        println(2)
        1 + 2
    }
    else {
        println(3)
        println(4)
        3 + 4
    }

    for (i in 1..10) {
        println(0)
        println(1)
        0 + 1
    }

    when (args.size) {
        0, 1 -> {
            println(1)
            println(1)
            println(1 + 1)
        }
        else -> {
            println(2)
            println(2)
            println(2 + 2)
        }
    }

    while (true) {
        println(5)
        println(6)
        println(5 + 6)
    }
}