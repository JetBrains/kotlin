class X(val x : Int) {}
class Y(val y : Int) {}

class Point(x : Int, y : Int) : X(x) , Y(y) {}

class Abstract {}

class P1(x : Int, yy : Y) : Abstract, X(x), Y by yy {}
class P2(x : Int, yy : Y) : X(x), Abstract, Y by yy {}
class P3(x : Int, yy : Y) : X(x), Y by yy, Abstract {}
class P4(x : Int, yy : Y) : Y by yy, Abstract, X(x) {}

