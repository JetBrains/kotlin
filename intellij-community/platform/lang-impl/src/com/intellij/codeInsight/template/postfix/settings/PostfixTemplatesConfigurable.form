            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <grid id="a9fc9" binding="myTemplatesTreeContainer" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
