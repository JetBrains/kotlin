// WITH_RUNTIME
fun test(x: Any): String {
    <caret>return when (x) {
        is String ->
            if (x.length > 3) "long string"
            else "short string"
        is Int ->
            if (x > 999 || x < -99) "long int"
            else "short int"
        is Long ->
            TODO()
        else ->
            "I don't know"
    }
}