import kotlin.internal.ExcludedFromFirstStageInlining

@ExcludedFromFirstStageInlining
inline fun inlineFunction() = "inlineFunction.v2"

@ExcludedFromFirstStageInlining
inline fun inlineFunctionWithParam(param: String = "default.v2") = "inlineFunctionWithParam.v2: $param"

@ExcludedFromFirstStageInlining
context(c: String)
inline fun String.inlineExtensionFunction() = "$this.inlineExtensionFunction.v2 with context $c"

class C {
    @ExcludedFromFirstStageInlining
    inline fun inlineClassFunction() = "inlineClassFunction.v2"

    @ExcludedFromFirstStageInlining
    inline fun inlineClassFunctionWithParam(param: String = "default.v2") = "inlineClassFunctionWithParam.v2: $param"

    @ExcludedFromFirstStageInlining
    context(c: String)
    inline fun String.inlineClassExtensionFunction() = "$this.inlineClassExtensionFunction.v2 with context $c"
}
