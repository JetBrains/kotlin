// PARAM_TYPES: V, Data
// PARAM_DESCRIPTOR: value-parameter val v: V defined in A.B.foo
open class Data(val x: Int)

class A<T: Data>(val t: T) {
    inner class B<U: Data>(val u: U) {
        // SIBLING:
        fun foo<V: Data>(v: V): Int {
            return i(v)
        }

        private fun <V : Data> i(v: V) = t.x + u.x + v.x
    }
}