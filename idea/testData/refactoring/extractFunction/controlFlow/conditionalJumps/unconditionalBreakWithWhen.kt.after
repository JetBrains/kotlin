// PARAM_TYPES: kotlin.Int
// SIBLING:
fun foo(a: Int): Int {
    val b: Int = 1
    for (n in 1..a) {
        unit(a, b)
        break
    }
    return 1
}

fun unit(a: Int, b: Int) {
    when {
        a + b > 0 -> return
        a - b > 0 -> return
        else -> {
            println(0)
            return
        }
    }
}