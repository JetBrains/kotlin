// RUN_PIPELINE_TILL: BACKEND
// ISSUE: KT-74824
// FILE: X.java

public class X<T1, T2> {
    public T1 a;
    public T2 b;

    public X(T1 a, T2 b) {
        this.a = a;
        this.b = b;
    }

    public <T3> X<X<T1, T2>, T3> with(T3 c) {
        return new X<>(this, c);
    }
}

// FILE: Example.kt

class Example {
    fun test() {
        X(1, 2)
            .with(3)
            .with(4)
            .with(5)
            .with(6)
            .with(7)
            .with(8)
            .with(9)
            .with(10)
            .with(11)
            .with(12)
            .with(13)
            .with(14)
            .with(15)
            .let { it }
    }
}
