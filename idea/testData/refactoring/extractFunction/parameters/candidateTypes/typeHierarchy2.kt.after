//PARAM_TYPES: C, B
//PARAM_DESCRIPTOR: value-parameter c: C defined in foo
interface A {
    fun doA()
}

open class AImpl: A {
    override fun doA() {
        throw UnsupportedOperationException()
    }
}

interface B {
    fun doB()
}

class C: AImpl(), B {
    override fun doA() {
        throw UnsupportedOperationException()
    }

    override fun doB() {
        throw UnsupportedOperationException()
    }

    fun doC() {
        throw UnsupportedOperationException()
    }
}

// SIBLING:
fun foo(c: C) {
    c.doA()
    __dummyTestFun__(c)
    c.doC()
}

private fun __dummyTestFun__(c: C) {
    c.doB()
}