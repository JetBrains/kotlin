fun box() : String {

  val hello="Hiram"
  val world="Chirino"

  val s = #\"
 ${hello}
 [\n]
 ${world}
 "

  if (s != " \${hello}\n [\\n]\n \${world}\n ") return "Fail: '$s'"

  val s2 = #"
 ${hello}
 [\n]
 ${world}
 "

  if (s2 != " Hiram\n [\n]\n Chirino\n ") return "Fail: '$s'"

  return "OK"
}