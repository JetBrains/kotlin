// PARAM_TYPES: kotlin.Int, Comparable<Int>
// PARAM_TYPES: kotlin.Int
// PARAM_DESCRIPTOR: value-parameter val a: kotlin.Int defined in foo
// PARAM_DESCRIPTOR: var b: kotlin.Int defined in foo
// SIBLING:
fun foo(a: Int): Int {
    var b: Int = 1

    b = i(a, b)

    return b
}

fun i(a: Int, b: Int): Int {
    var b1 = b
    when {
        a > 0 -> {
            b1 = b1 + 1
        }
        a < 0 -> {
            b1 = b1 - 1
        }
    }
    println(b1)
    return b1
}